const OpenForm1 = () => {
  document.getElementById("myForm1").style.display = "block";
  window.scrollTo({
      top: document.documentElement.scrollHeight,
      behavior: 'smooth',
  });
}
const CloseForm1 = () => {
  document.getElementById("myForm1").style.display = "none";
}
